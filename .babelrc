{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "node": 8
      },
      "modules": false
    }]
  ],
  "plugins": [
    [
      "@babel/transform-runtime",
      {
        "corejs": false,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }
    ]
  ]

}
